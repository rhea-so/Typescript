    "colors": "^1.4.0",
    "source-map-support": "^0.5.19"
  },
  "keywords": [
    "template"
  ],
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "typescript": "^4.1.3"
  }
}